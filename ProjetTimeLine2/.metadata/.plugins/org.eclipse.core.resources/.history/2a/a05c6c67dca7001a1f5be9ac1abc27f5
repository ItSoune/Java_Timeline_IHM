public class Client {

	public static void main(String[] args) {
		Frise a1 = new Frise();
		Date date = new Date(8,3,2011);
		Evt e1 = new Evt(date,"111","azer");
		Evt e5 = new Evt(date,"222","azer");
		Evt e3 = new Evt(new Date(10,6,2001),"333","azer");
